<%= title 'New Team Sprint' %>

<%= labelled_form_for @team_sprint, :url => url_for(:controller => 'team_sprints', :action => 'create') do |f| %>
  <%= render :partial => 'team_sprints/form', :locals => { :f => f } %>
  <%= submit_tag l(:button_create) %>
  <%= submit_tag l(:button_create_and_continue), :name => 'continue' %>
<% end %>
